{
  "name": "trusense-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode developmen",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --typecheck",
    "test:unit:coverage": "vitest run --coverage --typecheck",
    "test:integration": "vitest --typecheck",
    "test:e2e": "npx playwright test",
    "test:e2e:trace": "npx playwright test --trace on",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:create": "npx playwright codegen http://comic-generator.com.s3-website-us-east-1.amazonaws.com/index.html"
  },
  "dependencies": {
    "@kalimahapps/vue-icons": "^1.7.1",
    "@types/luxon": "^3.4.2",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-spinner": "^1.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "jsdom": "^24.1.0",
    "sass": "^1.77.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
